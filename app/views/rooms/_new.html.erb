새 룸 추가 폼

<%= form_for(@room, :url => rooms_path) do |f| %>
  <%= f.text_field :lat, :value => params[:lat] %>
  <%= f.text_field :lng, :value => params[:lng] %>

  <br>
  이름
  <%= f.text_field :name %>
  <br>
  종류
  <br>
  지역
  <br>
  주소
  <%= f.text_field :address %>
  <br>
  전화
  <%= f.text_field :phone %>
  <br>
  설명
  <%= f.text_area :description %>
  <br>
  그림
  <br>
  <%= link_to_function 'Submit', nil, :id => 'submit', :class => 'btn btn-inverse', :rel => '#new_room' %> 
<% end %>



