<div class="navbar">
  <div class="navbar-inner">
    <div class="container">
    <a class="brand" href="#">Project Oneroom</a>
    <div class="search_form search_basic">
      <div class="input-append">
      <%= text_field :search, :keyword %>
      <%= link_to_function 'Search', nil, :class => 'search_btn btn btn-inverse', :rel => '.search_basic' %>
      </div>
    </div>
    <div id="session">
    </div>
    <div>
      <% if @session -%>
        <% if @session.authorized -%>
          서울대인증회원입니다.
        <% else -%>
          <%= link_to '서울대인증하기', require_auth_user_path(@session) -%>
        <% end -%>
      <% end -%>
    </div>
  </div>
</div>

<div id="status" class="transparent">0</div>
